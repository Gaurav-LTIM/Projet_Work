<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idV4Languagekey"
        noDataText="No Languagekey Found"
        title="Select Languagekey"
        search=".onV4SearchLanguagekey"
        searchPlaceholder="Search Languagekey"
        confirm=".onV4LanguagekeyValueHelpDialogClose"
        cancel=".onValueHelpDialogClose"
        items="{ path: 'validationService>/ZMATERIALVH'}"    >
        <StandardListItem
            title="{validationService>LanguageKey}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>