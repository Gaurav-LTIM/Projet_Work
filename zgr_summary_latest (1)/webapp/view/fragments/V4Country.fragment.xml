<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idV4Country"
        noDataText="No Country/Region Found"
        title="Select Country/Region"
        search=".onV4SearchCountry"
        searchPlaceholder="Search Country/Region"
        confirm=".onV4CountryValueHelpDialogClose"
        cancel=".onValueHelpDialogClose"
        items="{ path: 'validationService>/ZFIXVENDORVH'}"    >
        <StandardListItem
            title="{validationService>Country}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>
