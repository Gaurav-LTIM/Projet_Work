<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idV4SearchTerm"
        noDataText="No SearchTerm Found"
        title="Select SearchTerm"
        search=".onV4SearchSearchTerm"
        searchPlaceholder="Search SearchTerm"
        confirm=".onV4SearchTermValueHelpDialogClose"
        cancel=".onValueHelpDialogClose"
        items="{ path: ''}"    >
        <StandardListItem
            title="{}"
            type="Active"
        />
    </SelectDialog>
</core:FragmentDefinition>