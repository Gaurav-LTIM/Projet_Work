<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog id ="idMaterial"
		noDataText="No Material Found"
		title="Select Material"
		search=".onSearchMaterial"
		searchPlaceholder="Search Material"
		confirm=".onMaterialValueHelpDialogClose"
		cancel=".onValueHelpDialogClose"
		items="{
			path: 'validationService>/ZMATERIALVH'
		}">
		<StandardListItem
			title="{validationService>MaterialCode}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>