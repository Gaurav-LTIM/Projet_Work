{
  "_version": "1.60.0",
  "sap.app": {
    "id": "com.sap.dashboard.db.idocdashboard",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.5",
      "toolsId": "f976e4d6-5c93-4c66-9059-381eed4daf46"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zui_idoc_list/srvd_a2x/sap/zui_outbound_idoc/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.viz": {},
        "sap.f": {},
				"sap.ui.integration": {},
				"sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.dashboard.db.idocdashboard.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.sap.dashboard.db.idocdashboard.view",
        "async": true,
        "viewPath": "com.sap.dashboard.db.idocdashboard.view"
      },
      "routes": [
        {
          "name": "RouteOverview",
          "pattern": ":?query:",
          "target": [
            "TargetOverview"
          ]
        },
        {
          "name": "Detailview",
          "pattern": "Detailview",
          "target": [
              "TargetDetailview"
          ]
        }
      ],
      "targets": {
        "TargetOverview": {
          "id": "Overview",
          "name": "Overview"
        },
        "TargetDetailview": {
          "id": "Detailview",
          "name": "Detailview"
        }
      }
    },
    "rootView": {
      "viewName": "com.sap.dashboard.db.idocdashboard.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
